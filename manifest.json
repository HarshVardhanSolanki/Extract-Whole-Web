{
  "manifest_version": 3,
  "name": "Extract Whole Web",
  "version": "1.0.4",
  "description": "Extract emails & mobile numbers from any keyword",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extract Whole Web"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["<all_urls>"]
}
